{
  "name": "cargurus-listings-fetcher",
  "version": "1.0.0",
  "description": "Fetch listings from cargurus.com for a particular make/model",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "test-ci": "npm run test && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Roilan/cargurus-listings-fetcher"
  },
  "keywords": [
    "cargurus",
    "cargurus api",
    "cargurus fetch"
  ],
  "author": "Roilan <roilan14@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Roilan/cargurus-listings-fetcher/issues"
  },
  "homepage": "https://github.com/Roilan/cargurus-listings-fetcher",
  "dependencies": {
    "form-data": "^2.3.2",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "codecov": "^3.1.0",
    "jest": "^23.6.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "collectCoverageFrom": [
    "**/*.{js}",
    "!**/node_modules/**"
  ]
}
